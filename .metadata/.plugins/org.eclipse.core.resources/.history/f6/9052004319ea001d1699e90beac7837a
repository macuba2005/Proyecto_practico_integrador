
package base;
public class persona {
	String nombre;
	public int Edad;

	  public persona() {
	    nombre = "Manuel";
	    Edad = 18;
	  }

	  public void setNombre(String nombre) {
	    this.nombre = nombre;
	  }

	  public String getNombre() {
	    return nombre;
	  }

	  public int getEdad() {
	    return Edad;
	  }
}
